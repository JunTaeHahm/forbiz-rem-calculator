{
    "name": "forbiz-rem-calculator",
    "displayName": "Forbiz REM Calculator",
    "description": "Multiply the entered pixel value by 2 and change it to rem in VSCode",
    "version": "0.1.0",
    "publisher": "JunTaeHahm",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.77.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JunTaeHahm/forbiz-rem-calculator.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.convertRem",
                "title": "Convert Rem"
            }
        ],
        "keybindings": [
            {
                "command": "extension.convertRem",
                "key": "alt+r",
                "mac": "cmd+r",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.77.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "eslint": "^8.36.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^4.9.5",
        "@vscode/test-electron": "^2.3.0"
    }
}
